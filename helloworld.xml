<Module>
  <ModulePrefs title="hello world example" /> 
  
   <Content type="html" view="profile">
	 
<![CDATA[ 

<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">

<div style="height:500px">
<h2 class="w3-center">Manual Slideshow</h2>

<div class="w3-content w3-display-container">

  <img class="mySlides" src="https://www.w3schools.com//w3css/img_fjords.jpg" style="width:100%;height:300px">
  <img class="mySlides" src="https://www.w3schools.com//w3css/img_lights.jpg" style="width:100%;height:300px">
  <img class="mySlides" src="https://www.w3schools.com//w3css/img_mountains.jpg" style="width:100%;height:300px">
  <img class="mySlides" src="https://www.w3schools.com//w3css/img_forest.jpg" style="width:100%;height:300px">

  <button class="w3-button w3-black w3-display-left" onclick="plusDivs(-1)">&#10094;</button>
  <button class="w3-button w3-black w3-display-right" onclick="plusDivs(1)">&#10095;</button>
</div>

<h2 class="w3-center">Manual Slideshow 3</h2>
</div>


<script>
var slideIndex = 1;
showDivs(slideIndex);

function plusDivs(n) {
  showDivs(slideIndex += n);
}

function showDivs(n) {
  var i;
  var x = document.getElementsByClassName("mySlides");
  if (n > x.length) {slideIndex = 1}    
  if (n < 1) {slideIndex = x.length}
  for (i = 0; i < x.length; i++) {
     x[i].style.display = "none";  
  }
  x[slideIndex-1].style.display = "block";  
}
</script>
]]>
	 
	 
	 
	 
